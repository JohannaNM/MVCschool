@model IEnumerable<MVCschool.Models.Enrollment>

@{
    ViewData["Title"] = "Index";
}

<h1>Programing 1</h1>

<table class="table table-bordered table-hover">
    <thead class="table-success">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th>
                Teacher
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.SFullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Course.Teacher.TFullName)
                </td>
            </tr>
        }
    </tbody>
</table>